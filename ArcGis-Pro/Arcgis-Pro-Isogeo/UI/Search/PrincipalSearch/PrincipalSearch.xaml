<UserControl x:Class="Arcgis_Pro_Isogeo.UI.Search.PrincipalSearch.PrincipalSearch"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Arcgis_Pro_Isogeo.UI.Search.PrincipalSearch"
             mc:Ignorable="d" 
             d:DesignHeight="27" d:DesignWidth="476">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <local:SearchItems Margin="0,0,5,0" Grid.Row="0" Grid.Column="0"/>
        <local:Keywords Margin="5,0,0,0"  Grid.Row="0" Grid.Column="1" />
    </Grid>
</UserControl>
