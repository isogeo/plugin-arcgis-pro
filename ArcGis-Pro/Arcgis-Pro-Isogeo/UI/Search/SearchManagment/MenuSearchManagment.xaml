             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Arcgis_Pro_Isogeo.UI.Search.SearchManagment"
             mc:Ignorable="d" 
             d:DesignHeight="24" d:DesignWidth="607">
    <Grid>
        <Menu HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <MenuItem x:Name="MniSearchManagement" Header="Search management">
