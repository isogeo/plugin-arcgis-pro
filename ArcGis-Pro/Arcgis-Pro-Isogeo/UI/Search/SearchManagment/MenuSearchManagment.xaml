                <MenuItem x:Name="MniPrecedentSearch" Header="Recherche précédente"></MenuItem>
                <MenuItem x:Name="MniSaveAs" Header="Enregistrer sous..."></MenuItem>
            </MenuItem>
        </Menu>
    </Grid>
</UserControl>
