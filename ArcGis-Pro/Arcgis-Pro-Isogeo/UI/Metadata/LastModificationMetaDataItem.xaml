<UserControl x:Class="Arcgis_Pro_Isogeo.UI.Metadata.LastModificationMetaDataItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Arcgis_Pro_Isogeo.UI.Metadata"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DataGrid CanUserAddRows="False"  
              AutoGenerateColumns="False"  
              HorizontalAlignment="Stretch" Margin="13,13,13,13" VerticalAlignment="Stretch">
        <DataGrid.Columns>
            <DataGridTextColumn Width="1*" Header="Date" />
            <DataGridTextColumn Width="1*" Header="Description"/>
        </DataGrid.Columns>
    </DataGrid>
</UserControl>
