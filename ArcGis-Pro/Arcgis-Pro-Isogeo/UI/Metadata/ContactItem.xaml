<UserControl x:Class="Arcgis_Pro_Isogeo.UI.ContactItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Arcgis_Pro_Isogeo.UI"
             mc:Ignorable="d" d:DesignWidth="317" Height="135">
    <StackPanel VerticalAlignment="Top">
        <Label x:Name="LblName" Content="lblName"/>
        <Label x:Name="LblEmail" Content="lblEmail" MouseDoubleClick="LblEmail_OnMouseDoubleClick" />
        <Label x:Name="LblPhone" Content="lblPhone"/>
        <Label x:Name="LblAddress" Content="lblAddress"/>
        <Label x:Name="LblCity" Content="lblCity"/>
    </StackPanel>
</UserControl>
