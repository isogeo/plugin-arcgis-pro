<UserControl x:Class="Arcgis_Pro_Isogeo.UI.Metadata.MetadataGeography"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Arcgis_Pro_Isogeo.UI.Metadata"
             mc:Ignorable="d" 
             d:DesignHeight="587" d:DesignWidth="602">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <GroupBox Grid.Row="0" Grid.Column="0" x:Name="GrpTechnicalInformation" Header="Informations techniques">
            <local:TechnicalInformationItem />
        </GroupBox>
        <GroupBox Grid.Row="1" Grid.Column="0" Header="Spécification">
            <Label x:Name="LblSpecification" Content="LblSpecification" />
        </GroupBox>
        <GroupBox x:Name="GrpTypology" Grid.Row="2" Grid.Column="0" Header="Typologie">
            <Label x:Name="LblTypology" Content="LblTypology" />
        </GroupBox>
    </Grid>
</UserControl>
